# Alternative GNU Make workspace makefile autogenerated by Premake

ifndef config
  config=debug
endif

ifndef verbose
  SILENT = @
endif

ifeq ($(config),debug)
  PKVRAgent_config = debug

else ifeq ($(config),release)
  PKVRAgent_config = release

else
  $(error "invalid configuration $(config)")
endif

PROJECTS := PKVRAgent

.PHONY: all clean help $(PROJECTS) 

all: $(PROJECTS)

PKVRAgent:
ifneq (,$(PKVRAgent_config))
	@echo "==== Building PKVRAgent ($(PKVRAgent_config)) ===="
	@${MAKE} --no-print-directory -C . -f PKVRAgent.make config=$(PKVRAgent_config)
endif

clean:
	@${MAKE} --no-print-directory -C . -f PKVRAgent.make clean

help:
	@echo "Usage: make [config=name] [target]"
	@echo ""
	@echo "CONFIGURATIONS:"
	@echo "  debug"
	@echo "  release"
	@echo ""
	@echo "TARGETS:"
	@echo "   all (default)"
	@echo "   clean"
	@echo "   PKVRAgent"
	@echo ""
	@echo "For more information, see https://github.com/premake/premake-core/wiki"